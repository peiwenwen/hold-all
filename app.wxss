/* 左右箭头 */
.icon-arrow {
  display: inline-block;
  padding: 6rpx;
  border: solid #333 1rpx;
  border-width: 0 1rpx 1rpx 0;
  vertical-align: 2rpx;
}
.icon-arrow-left {
  transform: rotate(135deg);
}
.icon-arrow-right {
  transform: rotate(-45deg);
}

/* 编辑 */
.icon-edit {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  background: no-repeat center / 36rpx url(data:image/png;base64,);
}

/* 删除 */
.icon-delete {
  display: inline-block;
  width: 48rpx;
  height: 48rpx;
  background: no-repeat center / 48rpx url(data:image/png;base64,);
}

/* 关闭 */
.icon-close {
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  background: no-repeat center / 30rpx url(data:image/png;base64,);
}

.icon-clear {
  margin-left: 24rpx;
  width: 36rpx;
  height: 36rpx;
  background: no-repeat center / 36rpx url(data:image/png;base64,);
}

.icon-tips {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  background: no-repeat center / 36rpx url(data:image/png;base64,);
  vertical-align: -2rpx;
}

.empty {
  height: 200rpx;
  text-align: center;
  color: #ccc;
  background: no-repeat center / 80rpx url(data:image/png;base64,);
}

.mask {
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  background-color: rgba(0,0,0,.5);
  z-index: 9;
}