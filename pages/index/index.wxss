.date-container {
  padding: 0 24rpx;
  font-size: 28rpx;
  color: #333;
}
.tools {
  position: fixed;
  top: 50%;
  right: 24rpx;
}
.icon {
  margin-bottom: 12rpx;
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  color: #fff;
  border-radius: 36rpx;
  font-size: 24rpx;
  background-color: #9dc3e2;
}

.switch-style {
  background-repeat: no-repeat;
  background-position: center;
  background-size: 56rpx;
  background-image: url(data:image/png;base64,);
}

.plan-list-all {
  background-repeat: no-repeat;
  background-position: center;
  background-size: 40rpx;
  background-image: url(data:image/png;base64,);
}