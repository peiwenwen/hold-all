<view class="todo-wrap">
  <block
    wx:for="{{todoList}}"
    wx:key="index">
    <view
      class="item {{!item.is ? 'disable' : ''}}"
      bindtap="goUrl"
      data-index="{{index}}">{{item.title}}
    </view>
  </block>
  <view class="egg-wrap" bindtap="egg">
    <view class="color-egg">
      <text class="color color1"></text>
      <text class="color color2"></text>
      <text class="color color3"></text>
      <text class="color color4"></text>
      <text class="color color5"></text>
    </view>
    <view class="text">
      <text class="font font1">c</text>
      <text class="font font2">o</text>
      <text class="font font3">l</text>
      <text class="font font4">o</text>
      <text class="font font5">r </text>
      <text class="font font1">e</text>
      <text class="font font2">g</text>
      <text class="font font3">g</text>
      <text class="font font4">~</text>
    </view>
  </view>
</view>
